      <CheckBooleanStep id="_14" client="$(client)" component="dialogS2_Resampling.saveToFileSelected" checktype="checked" resvarname="isSelected"/>
      <IfSequence id="_15" name="if" test="&quot;$(isSelected)&quot; == &quot;true&quot;">
        <MouseEventStep id="_16" clicks="1" client="$(client)" component="dialogS2_Resampling.saveToFileSelected" event="MOUSE_MPRC" modifiers="16"/>
