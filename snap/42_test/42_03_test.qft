      <ProcedureCall id="_7" procedure="snap.prepareTest"/>
      <ProcedureCall id="_8" procedure="products.open">
        <variable name="path">${env:test_data_dir}/s2tbx/S2/L1C/S2B_MSIL1C_20170315T013719_N0100_R000_T52LEK_20170315T013714.SAFE/MTD_MSIL1C.xml</variable>
      </ProcedureCall>
      <ProcedureCall id="_9" procedure="wait.product_is_opened"/>
      <MouseEventStep id="_10" clicks="1" client="$(client)" component="NbMainWindow.Optical" event="MOUSE_MPRC" modifiers="16"/>
      <MouseEventStep id="_11" clicks="1" client="$(client)" component="NbMainWindow.Geometric" event="MOUSE_MPRC" modifiers="16"/>
